{
  "lmStudio": {
    "clientOptions": {
      "baseUrl": "ws://127.0.0.1:1234"
    },
    "rest": {
      "baseUrl": "http://127.0.0.1:1234",
      "timeoutMs": 300000
    },
    "prompt": {
      "timeoutSeconds": 300,
      "noTokenTimeoutSeconds": 300,
      "navigator": {
        "timeoutSeconds": 300,
        "helperSilenceTimeoutMs": 12000
      }
    }
  },
  "prompt": {
    "timeoutSeconds": 300,
    "noTokenTimeoutSeconds": 300
  },
  "defaults": {
    "task": "Provide a precise technical analysis of the captured output.",
    "model": "mistralai/devstral-small-2-2512",
    "contextLength": 131072,
    "gpu": "auto",
    "timeoutSeconds": 3600,
    "summaryLevels": 3,
    "chunkSize": 2000,
    "sessionTimeoutSeconds": 1800
  },
  "rlRouter": {
    "enabled": false,
    "statePath": ".miniphi/indices/prompt-router.json",
    "models": [
      "mistralai/devstral-small-2-2512",
      "ibm/granite-4-h-tiny"
    ],
    "promptProfiles": [
      { "id": "default", "label": "Default" },
      { "id": "strict-json", "label": "Strict JSON", "prefix": "Reply with JSON only." }
    ]
  },
  "resourceMonitor": {
    "maxMemoryPercent": 85,
    "maxCpuPercent": 90,
    "maxVramPercent": 80,
    "sampleIntervalMs": 5000
  },
  "webBrowse": {
    "timeoutMs": 30000,
    "waitUntil": "domcontentloaded",
    "maxChars": 20000,
    "blockResources": true
  },
  "nitpick": {
    "writerModel": "microsoft/phi-4-reasoning-plus",
    "criticModel": "ibm/granite-4-h-tiny",
    "rounds": 2,
    "targetWords": 1200,
    "blind": false,
    "maxResults": 5,
    "maxSources": 6,
    "maxSourceChars": 1800,
    "researchRounds": 1,
    "provider": "duckduckgo",
    "browserTimeoutMs": 30000
  },
  "pythonScript": "./log_summarizer.py"
}
